{
  "name": "cv-webapp",
  "version": "0.0.0",
  "homepage": "https://davidduveau.github.io/cv-davidduveau/",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "node server/server.js",
    "dev:server": "node --watch server/server.js",
    "dev:all": "concurrently \"npm run dev:server\" \"npm run dev\"",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --config vitest.config.ts --root src/",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "deploy": "vite build && git add docs && git commit -m 'deploy' && git subtree push --prefix dist origin gh-pages"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.6.0",
    "axios": "^1.7.7",
    "jspdf": "^2.5.2",
    "pinia": "^2.2.6",
    "recharts": "^2.13.3",
    "vue": "^3.5.12",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.9",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.9.0",
    "@types/vue": "^2.0.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "gh-pages": "^6.2.0",
    "http-proxy-middleware": "^3.0.3",
    "jsdom": "^25.0.1",
    "nodemon": "^3.1.7",
    "npm-run-all2": "^7.0.1",
    "postcss": "^8.4.48",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-pwa": "^0.21.0",
    "vite-plugin-vue-devtools": "^7.5.4",
    "vitest": "^2.1.4",
    "vue-tsc": "^2.1.10"
  }
}
